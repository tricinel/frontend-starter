{
  "name": "frontend-starter",
  "version": "1.0.0",
  "description": "A quick starter project with all you need",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"./src/**/*.js\"",
    "prepublishOnly": "eslint-find-rules --unused",
    "test": "jest",
    "test:ci": "jest --runInBand --ci",
    "build": "babel src --out-dir dist"
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tricinel/frontend-starter.git"
  },
  "keywords": [
    "frontend",
    "starter",
    "config",
    "frontwerk",
    "javascript"
  ],
  "author": "Bogdan Lazar <bogdan@bogdanlazar.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tricinel/frontend-starter/issues"
  },
  "homepage": "https://github.com/tricinel/frontend-starter",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-frontwerk": "^1.0.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.1.3",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.4"
  },
  "engines": {
    "node": ">= 6"
  }
}
